{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "react-app",
    "prettier"
  ],
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "rules": {
    "jsx-quotes": [
      1,
      "prefer-double"
    ],
    "no-console": "warn"
  },
  "plugins": ["prettier", "react", "@typescript-eslint"]
  }